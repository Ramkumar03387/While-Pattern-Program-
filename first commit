public class PatternProgram{
public static void main(String[] args){
PatternProgram obj = new PatternProgram();
obj.methed1();
}
public void methed1(){
	int i = 1;
	int row = 6;
	while(row>=1){
	int col = 1;
	while(col<=i){
		System.out.print(" "+0+" ");
		col++;
	}
	int col1 = 10;
	while(col1>=i){
	  System.out.print(" "+" "+" "+" ");
	  col1--;
	}
	int col2 = 1;
	while(col2<=i){
	  System.out.print(" "+i+" ");
	  col2++;
	}
	System.out.println();
	row--;
	i++;
  }

}
}
